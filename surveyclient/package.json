{
  "name": "learning-play-audit",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@aws-amplify/auth": "^4.3.20",
    "@aws-amplify/core": "^4.3.14",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@svgr/webpack": "4.3.3",
    "axios": "^0.25.0",
    "camelcase": "^6.3.0",
    "chart.js": "^3.7.0",
    "fs-extra": "^10.0.0",
    "identity-obj-proxy": "3.0.0",
    "learning-play-audit-shared": "file:../sharedcode/learning-play-audit-shared-0.1.0.tgz",
    "localforage": "^1.9.0",
    "react": "^17.0.1",
    "react-app-polyfill": "^3.0.0",
    "react-chartjs-2": "^4.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "semver": "^7.3.5",
    "ts-pnp": "^1.2.0",
    "uuid": "^8.3.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "jest",
    "ftest": "wdio wdio.conf.js",
    "build:ltltest": "env-cmd -f .env.ltltest npm run build",
    "build:ltllive": "env-cmd -f .env.ltllive npm run build"
  },
  "eslintConfig": {
    "env": {
      "jest": true,
      "jest/globals": true
    },
    "extends": [
      "react-app",
      "plugin:jest/all"
    ],
    "plugins": [
      "jest"
    ],
    "rules": {
      "jest/require-top-level-describe": "off",
      "jest/prefer-expect-assertions": "off",
      "jest/no-hooks": "off",
      "jest/prefer-spy-on": "off",
      "jest/no-test-return-statement": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/../sharedcode/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "<rootDir>/../sharedcode/src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/register": "^7.12.1",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@wdio/cli": "^7.16.13",
    "@wdio/local-runner": "^7.16.13",
    "@wdio/mocha-framework": "^7.16.13",
    "@wdio/spec-reporter": "^7.16.13",
    "@wdio/sync": "^7.16.13",
    "babel-jest": "^27.4.6",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chromedriver": "^97.0.2",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "3.4.2",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.7.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^2.6.0",
    "file-loader": "4.3.0",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "jest": "^27.4.7",
    "jest-canvas-mock": "^2.3.1",
    "jest-resolve": "^27.4.6",
    "jest-watch-typeahead": "^1.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "^4.0.1",
    "react-dev-utils": "^11.0.3",
    "redux-mock-store": "^1.5.4",
    "resolve": "^1.22.0",
    "resolve-url-loader": "^5.0.0",
    "rfdc": "^1.3.0",
    "sass-loader": "8.0.2",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "2.3.0",
    "wdio-chromedriver-service": "^7.2.6",
    "webpack": "^4.42.0",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^6.0.2"
  }
}
