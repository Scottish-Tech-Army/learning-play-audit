{
  "name": "learning-play-audit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/auth": "^3.4.15",
    "@aws-amplify/core": "^3.8.7",
    "@babel/core": "7.9.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@svgr/webpack": "4.3.3",
    "axios": "^0.21.1",
    "camelcase": "^5.3.1",
    "chart.js": "^2.9.3",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "fs-extra": "^8.1.0",
    "identity-obj-proxy": "3.0.0",
    "learning-play-audit-shared": "file:../sharedmodel",
    "localforage": "^1.9.0",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "semver": "6.3.0",
    "ts-pnp": "1.1.6",
    "uuid": "^8.3.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "ftest": "wdio wdio.conf.js",
    "build:ltltest": "env-cmd -f .env.ltltest npm run build",
    "build:ltllive": "env-cmd -f .env.ltllive npm run build"
  },
  "eslintConfig": {
    "env": {
      "jest": true,
      "jest/globals": true
    },
    "extends": [
      "react-app",
      "plugin:jest/all"
    ],
    "plugins": [
      "jest"
    ],
    "rules": {
      "jest/require-top-level-describe": "off",
      "jest/prefer-expect-assertions": "off",
      "jest/no-hooks": "off",
      "jest/prefer-spy-on": "off",
      "jest/no-test-return-statement": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@babel/register": "^7.12.1",
    "@testing-library/dom": "^7.29.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "@wdio/cli": "^6.7.0",
    "@wdio/local-runner": "^6.7.0",
    "@wdio/mocha-framework": "^6.7.0",
    "@wdio/spec-reporter": "^6.7.0",
    "@wdio/sync": "^6.7.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chromedriver": "^86.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "3.4.2",
    "env-cmd": "^10.1.0",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "4.3.0",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "^4.0.1",
    "react-dev-utils": "^10.2.1",
    "redux-mock-store": "^1.5.4",
    "resolve": "^1.15.0",
    "resolve-url-loader": "^3.1.2",
    "rfdc": "^1.1.4",
    "sass-loader": "8.0.2",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "2.3.0",
    "wdio-chromedriver-service": "^6.0.4",
    "webpack": "^4.42.0",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^6.0.2"
  }
}
