{
  "name": "learning-play-audit",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@aws-amplify/auth": "^4.3.20",
    "@aws-amplify/core": "^4.3.14",
    "@babel/runtime": "^7.17.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "axios": "^0.25.0",
    "buffer": "^6.0.3",
    "camelcase": "^6.3.0",
    "chart.js": "^3.7.0",
    "fs-extra": "^10.0.0",
    "identity-obj-proxy": "3.0.0",
    "learning-play-audit-shared": "file:../sharedcode/learning-play-audit-shared-0.1.0.tgz",
    "learning-play-audit-survey": "file:../survey/learning-play-audit-survey-0.1.0.tgz",
    "localforage": "^1.9.0",
    "react": "^17.0.1",
    "react-app-polyfill": "^3.0.0",
    "react-chartjs-2": "^4.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "semver": "^7.3.5",
    "ts-pnp": "^1.2.0",
    "uuid": "^8.3.0"
  },
  "scripts": {
    "start": "npx webpack serve --config config/webpack.development.config.js",
    "build": "npx webpack --config config/webpack.production.config.js",
    "test": "jest",
    "ftest": "wdio wdio.conf.js",
    "build:ltltest": "env-cmd -f .env.ltltest npm run build",
    "build:ltllive": "env-cmd -f .env.ltllive npm run build"
  },
  "eslintConfig": {
    "env": {
      "jest": true,
      "jest/globals": true
    },
    "extends": [
      "react-app",
      "plugin:jest/all"
    ],
    "plugins": [
      "jest"
    ],
    "rules": {
      "jest/require-top-level-describe": "off",
      "jest/prefer-expect-assertions": "off",
      "jest/no-hooks": "off",
      "jest/prefer-spy-on": "off",
      "jest/no-test-return-statement": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/../sharedcode/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": [
        "babel-jest",
        {
          "presets": [
            "@babel/preset-env",
            "@babel/preset-react",
            [
              "@babel/preset-typescript",
              {
                "parserOpts": {
                  "strictMode": true
                }
              }
            ]
          ],
          "plugins": [
            "@babel/transform-runtime"
          ]
        }
      ],
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(learning-play-audit-shared|learning-play-audit-survey)/)"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "maxWorkers": "50%"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      [
        "@babel/preset-typescript",
        {
          "parserOpts": {
            "strictMode": true
          }
        }
      ]
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.12.1",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.1.0",
    "@types/jest": "^27.4.1",
    "@types/react-dom": "^17.0.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@wdio/cli": "^7.16.13",
    "@wdio/local-runner": "^7.16.13",
    "@wdio/mocha-framework": "^7.16.13",
    "@wdio/spec-reporter": "^7.16.13",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^10.0.1",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chromedriver": "^98.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.7.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.7",
    "jest-canvas-mock": "^2.3.1",
    "jest-resolve": "^27.4.6",
    "jest-watch-typeahead": "^1.0.0",
    "mini-css-extract-plugin": "^2.5.3",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.3.1",
    "postcss-safe-parser": "^6.0.0",
    "react-dev-utils": "^12.0.0",
    "resolve": "^1.22.0",
    "rfdc": "^1.3.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "typescript": "^4.6.3",
    "wdio-chromedriver-service": "^7.2.6",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-manifest-plugin": "^5.0.0",
    "workbox-webpack-plugin": "^6.0.2"
  }
}
